<!--pages/history/history.wxml-->
<import src="../../wxParse/wxParse.wxml" />
<view class="container">
  <view class="bg">
    <image src="../../images/bg.jpg"></image>
  </view>
  <view class="preface_box">
    <view class="preface_con">
      <view class="preface_con_box">
        <view class="history_box">

          <view  wx:for="{{data}}" wx:key="{{index}}">
            
          <view class="title2 flex-center">
            <view class="icon">
              <image src="../../images/left.png"></image>
            </view>
            <view class="h1">{{item.title}}</view>
            <view class="icon">
              <image src="../../images/right.png"></image>
            </view>
          </view> 
   
          <view class="history_con">
            <view class='wxParse'>
                <!-- <template is="wxParse" data="{{wxParseData:item}}"/> -->
                <rich-text nodes="{{item.content}}"></rich-text>
            </view>
          </view>
        </view> 

      </view>
      </view>
    </view>
  </view>
</view>