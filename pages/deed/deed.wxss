/* pages/deed/deed.wxss */
.container{
  height: calc(100% - 100rpx);
}
.preface_box{
  padding: 60rpx 35rpx 40rpx;
}

.practice_list{
  margin-top: 40rpx;
}
.practice_li{
  width: 48%;
  height: 174rpx;
  border-radius: 20rpx;
  margin-bottom: 30rpx;
}
.practice_li navigator{
  width: 100%;
  height: 100%;
}
.practice_li:first-child{
  background-image: linear-gradient(90deg,rgb(255,115,117),rgb(254,76,75));
}
.practice_li:nth-child(2){
  background-image: linear-gradient(90deg,rgb(67,117,253),rgb(53,104,255));
}
.practice_li:nth-child(3){
  background-image: linear-gradient(90deg,rgb(12,213,170),rgb(11,191,164));
}
.practice_li:nth-child(4){
  background-image: linear-gradient(90deg,rgb(255,168,0),rgb(255,144,0));
}
.practice_li .icon{
  width: 81rpx;
  height: 81rpx;
  font-size: 0;
}
.practice_li .icon image{
  width: 100%;
  height: 100%;
}
.practice_li .h1{
  font-size: 26rpx;
  color: #fff;
  margin-top: 20rpx;
}